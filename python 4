# Função que mostra se o número é primo ou não.
def numero_primo(num):
    if num< 2 :
        return False
    for i in range(2, num):
        if num % i == 0:
            return False
        return True
    # imprimir todos numeros primos de 1 a 100.
for num in range(0, 100):
    if numero_primo(num):
        print(f"{num} é primo")

